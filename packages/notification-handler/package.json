{
  "name": "@feather/notification-handler",
  "version": "0.0.1",
  "private": true,
  "exports": {
    "./*": "./src/*.tsx",
    "./styles.css": "./dist/index.css"
  },
  "files": [
    "dist"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "scripts": {
    "lint": "eslint . --max-warnings 0",
    "build": "tsc",
    "build:styles": "tailwindcss -i ./src/styles.css -o ./dist/index.css",
    "build:components": "tsc",
    "check-types": "tsc --noEmit",
    "dev:styles": "tailwindcss -i ./src/styles.css -o ./dist/index.css --watch",
    "dev:components": "tsc --watch"
  },
  "devDependencies": {
    "@feather/eslint-config": "workspace:*",
    "@feather/tailwind-config": "workspace:*",
    "@feather/typescript-config": "workspace:*",
    "@tailwindcss/cli": "^4.1.11",
    "@turbo/gen": "^2.5.0",
    "@types/node": "^22.15.3",
    "@types/react": "19.1.0",
    "@types/react-dom": "19.1.1",
    "eslint": "^9.29.0",
    "tailwindcss": "^4.1.11",
    "typescript": "5.8.2"
  },
  "dependencies": {
    "@gsap/react": "^2.1.2",
    "gsap": "^3.13.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  }
}
